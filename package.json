{
  "name": "@easy-team/easywebpack-cli",
  "version": "4.4.1",
  "description": "Webpack Building Command Line And Boilerplate Init Tool",
  "bin": {
    "easywebpack": "bin/cli.js",
    "easy": "bin/cli.js"
  },
  "scripts": {
    "lint": "eslint .",
    "fix": "eslint --fix .",
    "changelog": "conventional-changelog -p angular -i CHANGELOG.md -s",
    "test-mocha": "mocha test --timeout 15000 --require co-mocha",
    "test": "nyc --reporter=html --reporter=text npm run test-mocha",
    "cov": "nyc report --reporter=lcov > coverage.lcov && codecov --token=d7567590-d31c-4c14-8707-574d8ab10c03",
    "ci": "npm run lint && npm run cov",
    "ii": "npm install --registry https://registry.npm.taobao.org",
    "postinstall": "node ./lib/postinstall.js"
  },
  "dependencies": {
    "archive-tool": "^1.0.0",
    "babel-upgrade": "^1.0.1",
    "chalk": "^2.0.1",
    "commander": "^2.11.0",
    "compressing": "^1.2.3",
    "easy-puppeteer-html": "^1.0.0",
    "execa": "^1.0.0",
    "inquirer": "^7.0.1",
    "jsdom": "^15.2.1",
    "lodash.get": "^4.4.2",
    "mz-modules": "^1.0.0",
    "node-glob": "^1.2.0",
    "node-tool-utils": "^1.0.0",
    "ora": "^3.0.0",
    "progress-bar-webpack-plugin": "^1.11.0",
    "shelljs": "^0.7.8",
    "speed-measure-webpack-plugin": "^1.2.2",
    "stats-webpack-plugin": "^0.7.0",
    "urllib": "^2.24.0",
    "webpack-bundle-analyzer": "^3.0.2",
    "webpack-tool": "^4.2.0"
  },
  "devDependencies": {
    "chai": "^4.1.0",
    "co-mocha": "^1.2.0",
    "codecov": "^2.3.0",
    "conventional-changelog-cli": "^1.3.5",
    "eslint": "^4.3.0",
    "istanbul": "^0.4.5",
    "mocha": "^3.5.0",
    "nyc": "^11.1.0"
  },
  "files": [
    "index.js",
    "bin",
    "lib",
    "tool"
  ],
  "nyc": {
    "exclude": [
      "**/*.spec.js",
      "bin/cli.js",
      "lib/builder.js",
      "lib/command.js",
      "index.js",
      "test/*.test.js"
    ]
  },
  "engines": {
    "node": ">=6.0.0"
  },
  "ci": {
    "version": "6, 8, 10"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/easy-team/easywebpack-cli.git"
  },
  "author": "hubcarl@126.com",
  "license": "MIT",
  "homepage": "https://github.com/easy-team/easywebpack-cli"
}